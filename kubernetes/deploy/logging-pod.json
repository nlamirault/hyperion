{
    "id": "logging",
    "kind": "Pod",
    "apiVersion": "v1beta1",
    "desiredState": {
        "manifest": {
            "version": "v1beta1",
            "id": "logging",
            "containers": [
                {
                    "name": "logging-metrics",
                    "image": "hyperion-logging-metrics",
                }, {
	            "name": "logging-ui",
                    "image": "hyperion-logging-ui",
                    "ports": [{
                        "containerPort": 80,
                        "hostPort": 80
                    }]
                }, {
                    "name": "logging-redis",
                    "image": "dockerfile/redis",
                    "ports": [{
                        "containerPort": 6379,
                        "hostPort": 6379
                    }]
                }
            ]
        },
    },
    "labels": {
        "name": "hyperion-logging",
    }
}
