[Unit]
Description=Etcd Server
After=network.target

[Service]
Type=simple
StandardOutput=journal
StandardError=journal
ExecStart=/usr/bin/etcd --data-dir=/var/lib/etcd

[Install]
WantedBy=multi-user.target

./etcd -name kmaster1 -initial-advertise-peer-urls http://10.27.100.9:2380 -listen-peer-urls http://10.27.100.9:2380 -listen-client-urls http://10.27.100.9:2379,http://127.0.0.1:2379 -advertise-client-urls http://10.27.100.9:2379 -initial-cluster-token etcd-cluster-1 -initial-cluster kmaster1=http://10.27.100.9:2380,kmaster2=http://10.27.100.10:2380,kmaster3=http://10.27.100.11:23801
